{
    "env": {
        "browser": true,
        "es6": true,
        "webextensions": true,
        "jquery": true,
        "worker": true
    },
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
        "no-console": 1
  }
}
