{
  "name": "trackingtransparency",
  "version": "0.0.7",
  "description": "Provides information about online tracking",
  "scripts": {
    "build": "wp --progress --config webpack.dev.js",
    "chromium": "node test/chromium.js",
    "dist": "wp --progress --config webpack.prod.js && web-ext build -s extension/ --overwrite-dest",
    "firefox": "web-ext run -s extension/ -u 'about:debugging'",
    "jsdoc": "jsdoc -c jsdoc.conf.json .",
    "lint": "npm run lint:js; npm run lint:webext",
    "lint:js": "standardx",
    "lint:webext": "web-ext lint -s extension/",
    "preversion": "npm run build && npm run test",
    "start": "wp -w --progress --config webpack.dev.js",
    "test": "tape test/test.js | tap-spec",
    "version": "file-version-bump",
    "watch": "wp -w --progress --config webpack.dev.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-eval": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "file-version-bump": "0.0.2",
    "foodoc": "^0.0.9",
    "gzip-loader": "^0.0.1",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^0.4.2",
    "puppeteer": "^1.6.1",
    "standardx": "^2.1.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "web-ext": "^2.9.1",
    "webpack": "^4.17.2",
    "webpack-command": "^0.4.1",
    "webpack-merge": "^4.1.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@instructure/ui-alerts": "^5.25.0",
    "@instructure/ui-breadcrumb": "^5.25.0",
    "@instructure/ui-buttons": "^5.25.0",
    "@instructure/ui-elements": "^5.25.0",
    "@instructure/ui-forms": "^5.25.0",
    "@instructure/ui-icons": "^5.25.0",
    "@instructure/ui-layout": "^5.25.0",
    "@instructure/ui-overlays": "^5.25.0",
    "@instructure/ui-themes": "^5.25.0",
    "@instructure/ui-toggle-details": "^5.25.0",
    "babel-runtime": "^6.26.0",
    "lodash": "^4.17.10",
    "lovefield": "^2.1.12",
    "moment": "^2.22.2",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.3",
    "react-d3-cloud": "^0.6.0",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.4.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-sizeme": "^2.5.2",
    "react-table": "^6.8.5",
    "react-vis": "^1.10.8",
    "snowball": "^0.3.1",
    "tldjs": "^2.3.1",
    "webextension-polyfill": "^0.3.1"
  },
  "standardx": {
    "globals": [
      "browser",
      "chrome"
    ],
    "ignore": [
      "doc/",
      "extension/lib/",
      "extension/dist/",
      "src/lightbeam/"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    }
  },
  "file-version-bump": [
    "extension/manifest.json"
  ]
}
