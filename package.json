{
  "name": "trackingtransparency",
  "version": "1.0.0",
  "description": "Provides information about online tracking",
  "scripts": {
    "test": "cd test && node test.js | tap-spec",
    "lint:eslint": "eslint .",
    "lint:webext": "web-ext lint -s extension/",
    "lint": "npm run lint:eslint; npm run lint:webext",
    "dist": "webpack --progress --config webpack.prod.js && web-ext build -s extension/ --overwrite-dest",
    "start": "webpack -w --progress --config webpack.dev.js",
    "watch": "webpack -w --progress --config webpack.dev.js",
    "build": "webpack --progress --config webpack.dev.js",
    "firefox": "web-ext run -s extension/ -u 'about:debugging'",
    "jsdoc": "jsdoc -c jsdoc.conf.json -r -d ./doc/jsdoc/ -R ./README.md ."
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-eval": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.6",
    "jsdoc": "^3.5.5",
    "puppeteer": "^1.0.0",
    "style-loader": "^0.19.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0",
    "url-loader": "^0.6.2",
    "web-ext": "^2.3.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "html-entities": "^1.2.1",
    "lodash": "^4.17.4",
    "lovefield": "^2.1.12",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.7.6",
    "react-vis": "^1.8.1",
    "striptags": "^3.1.1",
    "tokenize-text": "^1.1.3",
    "webextension-polyfill": "^0.2.1"
  }
}
