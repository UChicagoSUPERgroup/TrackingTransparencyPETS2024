{
  "name": "trackingtransparency",
  "version": "1.0.0",
  "description": "Provides information about online tracking",
  "scripts": {
    "test": "cd test && node test.js | tap-spec",
    "lint:eslint": "eslint src",
    "lint:webext": "web-ext lint -s extension/",
    "lint": "npm run lint:eslint; npm run lint:webext",
    "dist": "wp --progress --config webpack.prod.js && web-ext build -s extension/ --overwrite-dest",
    "start": "wp -w --progress --config webpack.dev.js",
    "watch": "wp -w --progress --config webpack.dev.js",
    "build": "wp --progress --config webpack.dev.js",
    "firefox": "web-ext run -s extension/ -u 'about:debugging'",
    "jsdoc": "jsdoc -c jsdoc.conf.json ."
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-eval": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "foodoc": "^0.0.9",
    "gzip-loader": "^0.0.1",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^0.4.1",
    "puppeteer": "^1.6.0",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "web-ext": "^2.6.0",
    "webpack": "^4.16.0",
    "webpack-command": "^0.4.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/react-fontawesome": "file:node_modules/@fortawesome/react-fontawesome",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "html-entities": "^1.2.1",
    "lodash": "^4.17.10",
    "lovefield": "^2.1.12",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-d3-cloud": "^0.6.0",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.8.5",
    "react-vis": "^1.10.3",
    "striptags": "^3.1.1",
    "tldjs": "^2.3.1",
    "tokenize-text": "^1.1.3",
    "webextension-polyfill": "^0.2.1"
  },
  "standard": {
    "ignore": [
      "doc/",
      "extension/lib/",
      "extension/dist/",
      "src/lightbeam/"
    ]
  }
}
