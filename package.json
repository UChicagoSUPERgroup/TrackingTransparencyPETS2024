{
  "name": "trackingtransparency",
  "version": "1.0.0",
  "description": "Provides information about online tracking",
  "scripts": {
    "test": "karma start --browsers Firefox,Chrome --single-run",
    "lint:eslint": "eslint .",
    "lint:webext": "web-ext lint -s extension/",
    "lint": "npm run lint:eslint; npm run lint:webext",
    "dist": "webpack --progress --config webpack.prod.js && web-ext build -s extension/",
    "start": "webpack -w --progress --config webpack.dev.js",
    "watch": "webpack -w --progress --config webpack.dev.js",
    "build": "webpack --progress --config webpack.dev.js",
    "firefox": "web-ext run -s extension/ -u 'about:debugging'",
    "jsdoc": "jsdoc -c jsdoc.conf.json -r -d ./doc/jsdoc/ -R ./README.md ."
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-eval": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.6",
    "jsdoc": "^3.5.5",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.4.2",
    "sinon-chrome": "^2.2.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "web-ext": "^2.2.2",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "d3": "^4.12.0",
    "html-entities": "^1.2.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "lovefield": "^2.1.12",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-vis": "^1.8.0",
    "readability": "github:mozilla/readability",
    "striptags": "^3.1.1",
    "tokenize-text": "^1.1.3",
    "typeface-source-sans-pro": "^0.0.44",
    "webextension-polyfill": "^0.2.1"
  }
}
